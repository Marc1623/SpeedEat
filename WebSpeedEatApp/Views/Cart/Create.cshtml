@model WebSpeedEatApp.Models.Orders_Dishes
@{
    ViewData["Title"] = "Create Orders";
}
<h2>Create</h2>
<h4>Orders</h4>
<hr />
<form asp-action="Create" class="form-horizontal">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label class="control-label">IdOders_Dishes</label>
        <input asp-for="IdOders_Dishes" class="form-control" />
        <span asp-validation-for="IdOders_Dishes" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Oders_DishesQuantity</label>
        <input asp-for="Oders_DishesQuantity" class="form-control" />
        <span asp-validation-for="Oders_DishesQuantity" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Oders_DishesCreated_At</label>
        <input asp-for="Oders_DishesCreated_At" class="form-control" />
        <span asp-validation-for="Oders_DishesCreated_At" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Oders_DishesFk_Id_Orders</label>
        <input asp-for="Oders_DishesFk_Id_Orders" class="form-control" />
        <span asp-validation-for="Oders_DishesFk_Id_Orders" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="control-label">Oders_DishesFk_Id_Dishes</label>
        <input asp-for="Oders_DishesFk_Id_Dishes" class="form-control" />
        <span asp-validation-for="Oders_DishesFk_Id_Dishes" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Submit" class="btn bg-primary" />

    </div>
</form>
@{
    if (@TempData["Msg"] != null)
    {
        <script>

            alert('@TempData["msg"]')
        </script>

    }
}
